{{ header }}
<div class="wrapper">
    <div class="header">
        <div class="row no-gutters">
            <div class="col-auto">
                <button class="btn  btn-link text-dark menu-btn"><img src="catalog/view/theme/go/image/menu.png" alt=""><span
                            class="new-notification"></span></button>
            </div>
            <div class="col text-center"><h3 class="header-logo mt-3">OBKE</h3></div>
            <div class="col-auto">
                <a href="#" class="btn  btn-link text-dark"><i class="material-icons">account_circle</i></a>
            </div>
        </div>
    </div>
    <div id="error-not-found" class="container">
        <div class="row">
            <div id="content" class="col">
                <h1>{{ heading_title }}</h1>
                <p class="alert alert-danger">{{ text_error }}</p>
                <div class="d-inline-block pt-2 pd-2 w-100">
                    <div class="float-right"><a href="{{ continue }}" class="btn btn-primary">{{ button_continue }}</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    {{ footer }}
</div>
